<Meta::Tags>
	<title>SurrealDB | Media | The ultimate multi-model database</title>
	<meta name="title" property="og:title" content="SurrealDB | Media">
	<meta name="description" content="Download images and videos for use in press releases, blog articles, or social posts. For media inquires and more information, email us at info@surrealdb.com.">
</Meta::Tags>

<Layout::Hero media>
	<Layout::Text text-l>
		<h1>Media</h1>
		<h2>For media inquires and more information, email us at <a href="mailto:info@surrealdb.com">info@surrealdb.com</a>.</h2>
		<p>
			<Link @link="#colours">Colours</Link>
			&bull;
			<Link @link="#logosandicons">Logos and icons</Link>
			&bull;
			<Link @link="#poweredby">'Powered by' badges</Link>
			&bull;
			<Link @link="#builton">'Built on' badges</Link>
		</p>
	</Layout::Text>
</Layout::Hero>

<Layout::Gap mini />

<section id="colours">

	<Layout::Gap small />

	<Layout::Text text-l>
		<h2>Colours</h2>
	</Layout::Text>

	<Layout::Gap mini />

	<Layout::Blocks cols-3-to-2 as |block|>
		<block>
			<div curved bg-ff00a0 w-100 h-60px m-b-20></div>
			<h4 flex row middle>#ff00a0</h4>
		</block>
		<block>
			<div curved bg-9600ff w-100 h-60px m-b-20></div>
			<h4 flex row middle>#9600ff</h4>
		</block>
		<block>
			<div curved bg-ffffff w-100 h-60px m-b-20></div>
			<h4 flex row middle>#ffffff</h4>
		</block>
		<block>
			<div curved bg-1a202d w-100 h-60px m-b-20></div>
			<h4 flex row middle>#151517</h4>
		</block>
		<block>
			<div curved bg-272e3c w-100 h-60px m-b-20></div>
			<h4 flex row middle>#f5f5f7</h4>
		</block>
	</Layout::Blocks>

	<Layout::Gap small />

</section>

<section id="logosandicons">

	<Layout::Gap small />

	<Layout::Text text-l>
		<h2>Logos and icons</h2>
	</Layout::Text>

	<Layout::Gap mini />

	<Layout::Blocks cols-3-to-2-to-1 as |block|>
		<block>
			<grid p-30 flex row center middle>
				<img max-h-60 loading="lazy" src="/static/img/assets/logo/light.svg" alt="SurrealDB light logo" />
			</grid>
			<a m-t-10 m-r-10 href="/static/img/assets/logo/light.svg" download="surrealdb-logo-light">
				<i class="fal fa-cloud-download"></i> SVG
			</a>
			<a m-t-10 m-r-10 href="/static/img/assets/logo/light.png" download="surrealdb-logo-light">
				<i class="fal fa-cloud-download"></i> PNG
			</a>
		</block>
		<block>
			<grid p-30 flex row center middle>
				<img max-h-60 loading="lazy" src="/static/img/assets/logo/dark.svg" alt="SurrealDB dark logo" />
			</grid>
			<a m-t-10 m-r-10 href="/static/img/assets/logo/dark.svg" download="surrealdb-logo-dark">
				<i class="fal fa-cloud-download"></i> SVG
			</a>
			<a m-t-10 m-r-10 href="/static/img/assets/logo/dark.png" download="surrealdb-logo-dark">
				<i class="fal fa-cloud-download"></i> PNG
			</a>
		</block>
		<block>
			<grid p-30 flex row center middle>
				<img max-h-60 loading="lazy" src="/static/img/assets/icon/icon.svg" alt="SurrealDB icon" />
			</grid>
			<a m-t-10 m-r-10 href="/static/img/assets/icon/icon.svg" download="surrealdb-icon">
				<i class="fal fa-cloud-download"></i> SVG
			</a>
			<a m-t-10 m-r-10 href="/static/img/assets/icon/icon.png" download="surrealdb-icon">
				<i class="fal fa-cloud-download"></i> PNG
			</a>
		</block>
	</Layout::Blocks>

	<Layout::Gap small />

</section>

